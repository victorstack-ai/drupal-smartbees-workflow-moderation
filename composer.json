{
  "name": "victorstack-ai/drupal-smartbees-workflow-moderation",
  "description": "Drupal module providing a Smartbees content moderation workflow.",
  "type": "drupal-module",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/yaml": "^6.3 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Smartbees\\WorkflowModeration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smartbees\\WorkflowModeration\\Tests\\": "tests/"
    }
  }
}
